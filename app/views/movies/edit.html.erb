<h1>Editing <%= @movie.title %></h1>

<%= render "form", movie: @movie %>

<section class="admin">
  <%= link_to 'Edit', edit_movie_path(@movie), class: 'button' %>
  <%= link_to 'Delete', movie_path(@movie), class: 'button',
                method: :delete,
                data: { confirm: 'Are you sure?' } %>
</section>
